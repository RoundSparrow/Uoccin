    <string-array name="view_defser_queries">
        <!-- serfavs -->
        <item>
select tvdb_id from series where watchlist = 1 order by name collate nocase
        </item>
        <!-- sernext -->
        <item>
select distinct s.tvdb_id from series s join episode e on (e.series = s.tvdb_id) 
where datetime(e.firstAired/1000, \'unixepoch\') between datetime(\'now\') 
and datetime(\'now\', \'+6 days\') and (s.watchlist = 1 or (e.season = 1 and e.episode = 1)) 
order by e.firstAired
        </item>
        <!-- ser2see -->
        <item>
select distinct s.tvdb_id from series s join episode e on (e.series = s.tvdb_id) 
where e.collected = 1 and e.watched = 0 order by s.name collate nocase
        </item>
        <!-- sernews -->
        <item>
select distinct s.tvdb_id from series s join episode e on (e.series = s.tvdb_id) 
where datetime(e.firstAired/1000, \'unixepoch\') between datetime(\'now\', \'-7 days\') 
and datetime(\'now\', \'+7 days\') and e.season = 1 and e.episode = 1 order by e.firstAired
        </item>
        <!-- serstat -->
        <item>
select distinct s.tvdb_id from series s join episode e on (e.series = s.tvdb_id) 
order by s.name collate nocase
        </item>
    </string-array>
    
